<Window x:Class="newOW2TrackerWithAPI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:newOW2TrackerWithAPI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="13*"/>
        </Grid.RowDefinitions>
        <Label x:Name="userNameLabel" Content="UerName" HorizontalAlignment="Left" Margin="92,12,0,0" VerticalAlignment="Top" HorizontalContentAlignment="Left" FontSize="36" FontWeight="Bold" Grid.Row="1"/>
        <Image x:Name="WebImage" HorizontalAlignment="Left" Height="100" Margin="320,70,0,0" VerticalAlignment="Top" Width="100" Grid.Row="1"/>
        <Image x:Name="namecardImage" HorizontalAlignment="Left" Height="100" Margin="68,27,0,0" VerticalAlignment="Top" Width="235" Grid.RowSpan="2"/>
        <Image HorizontalAlignment="Left" Height="100" Margin="360,96,0,0" VerticalAlignment="Top" Width="100" Grid.Row="1"/>
        <Button x:Name="buttontest" Content="Button" HorizontalAlignment="Left" Margin="320,118,0,0" VerticalAlignment="Top" Click="Button_Click" Grid.Row="1"/>
      
    </Grid>
</Window>
